    <field name="ProductLength" type="Decimal" isPrimaryKey="true" label="Product Length" />
    <field name="ConsumedOn" type="DateTime" allowNulls="false" isPrimaryKey="true" label="Consumed On" showInSummary="true" />
    <field name="Packets" type="Int32" label="Packets" showInSummary="true" />
  </fields>
