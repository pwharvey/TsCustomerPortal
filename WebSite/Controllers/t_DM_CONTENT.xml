	,"t_DM_CONTENT"."LAST_VERSION_IND" "LAST_VERSION_IND"
	,"t_DM_CONTENT"."CTRL_USER_ID" "CTRL_USER_ID"
	,"t_DM_CONTENT"."CTRL_CRE_DATE" "CTRL_CRE_DATE"
	,"t_DM_CONTENT"."CTRL_UPD_DATE" "CTRL_UPD_DATE"
	,"t_DM_CONTENT"."CTRL_APP_NAME" "CTRL_APP_NAME"
	,"t_DM_CONTENT"."SECURITY_PROF" "SECURITY_PROF"
	,"t_DM_CONTENT"."NOTES" "NOTES"
from "dbo"."t_DM_CONTENT" "t_DM_CONTENT"
]]></text>
    </command>
  </commands>
